<ul class="mfriend">
{{#each data}}
	<li action-type="show-chat" data-create="create" data-cid="{{../openId}}:{{openId}}">
		<div class="media">
		  <div class="media-left">
		      <img class="media-object itu-img" src="http://u1.renhe.cn/userface/{{member.userImg}}" alt="{{member.name}}">
		  </div>
		  <div class="media-body">
		    <h4 class="media-heading">{{member.name}}<span>{{member.curTitle}}</span></h4>
			<p>{{member.curCompany}}</p>
		  </div>
		</div>
	</li>
{{/each}}
</ul>
